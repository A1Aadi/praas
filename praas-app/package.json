{
  "name": "@conduits-xyz/conduits.app",
  "version": "0.3.1",
  "description": "web.app for managing conduits to non-traditional storage",
  "author": "conduits.xyz",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@github.com:million-views/praas.git"
  },
  "keywords": [
    "proxy",
    "serverless",
    "endpoint",
    "nts",
    "conduit"
  ],
  "scripts": {
    "lint": "stylelint src/**/*.{css,scss} --cache && eslint src --cache",
    "lint:fix": "stylelint src/**/*.{css,scss} --fix && eslint --fix src",
    "build": "webpack --colors --progress -p --env.production --config src/cfg/webpack",
    "watch": "webpack-dev-server --progress --config src/cfg/webpack",
    "test": "npm run -s lint && jest -c src/cfg/jest.config.js --no-cache --coverage --rootDir .",
    "test:watch": "npm run -s lint && jest -c src/cfg/jest.config.js --watch --no-cache --rootDir .",
    "start": "npm run build && node ./live-server build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix src"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.11.1",
    "formik": "^2.1.5",
    "history": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "stylelint-config-recommended": "^3.0.0",
    "tsutils": "^3.17.1",
    "typescript": "^3.9.7",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.8",
    "@testing-library/user-event": "^12.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.2.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "core-js": "^3.6.5",
    "css-loader": "^4.2.0",
    "css-url-loader": "2.0.2",
    "eslint": "^7.6.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jest-dom": "^3.1.6",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-testing-library": "^3.4.1",
    "file-loader": "^6.0.0",
    "html-inline-css-webpack-plugin": "^1.8.1",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "msw": "^0.20.3",
    "node-fetch": "2.6.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^9.0.2",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "suppress-chunks-webpack-plugin": "^1.0.6",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.0"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 safari versions",
      "last 2 edge versions"
    ],
    "production": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 safari versions",
      "last 2 edge versions"
    ]
  },
  "engines": {
    "node": ">=14"
  }
}